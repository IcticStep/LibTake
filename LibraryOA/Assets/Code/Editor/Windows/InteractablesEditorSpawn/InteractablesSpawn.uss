.do-button {
    background-color: rgba(233, 198, 102, 0.45);
    -unity-background-scale-mode: stretch-to-fill;
    background-image: none;
    font-size: 16px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-left-color: rgb(7, 7, 6);
    border-right-color: rgb(7, 7, 6);
    border-top-color: rgb(7, 7, 6);
    border-bottom-color: rgb(7, 7, 6);
}
